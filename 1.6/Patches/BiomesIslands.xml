<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Islands</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="BiomesIslands_AquaticKelpBed" or defName="BMT_AquaticFence" or defName="BMT_AquaticFenceGate"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_AnimalHandling</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="BMT_Apparel_Flippers"]/recipeMaker</xpath>
					<value>
						<researchPrerequisite>VFET_Tribalwear</researchPrerequisite>
					</value>
				</li>


			</operations>
		</match>
	</Operation>
</Patch>