<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Core</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/TerrainDef[@Name="BiomesCore_FloatingGarden"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Agriculture</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/TerrainDef[defName="BMT_PontoonBridge"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Construction</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>