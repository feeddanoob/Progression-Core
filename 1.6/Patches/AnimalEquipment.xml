<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Animal Equipment</li>
        </mods>
		
        <match Class="PatchOperationSequence">
            <!--<success>Always</success>-->
            <operations>


<li Class="PatchOperationRemove">
  <xpath>Defs/ThingDef[@Name="AnimalScarfBase"]/recipeMaker/researchPrerequisite</xpath>  
</li>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[@Name="AnimalScarfBase"]/recipeMaker</xpath>
  <value>
	<researchPrerequisite>VFET_Tribalwear</researchPrerequisite>
  </value>
</li>

</operations>
</match>
</Operation>

</Patch>