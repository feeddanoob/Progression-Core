<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- Some sample patches. Note you may have to use Add or Replace depending on what's already in the def -->

<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Igloo</li>
        </mods>
		
        <match Class="PatchOperationSequence">
            <!--<success>Always</success>-->
            <operations>

<!-- Buildings -->             		

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Tent_Igloo"]</xpath>
  <value>
   <researchPrerequisites>
			<li>VFET_Furniture</li>
		</researchPrerequisites>
  </value>
</li>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Addon_Ski"]</xpath>
  <value>
   <researchPrerequisites>
			<li>VFET_Furniture</li>
		</researchPrerequisites>
  </value>
</li>

</operations>
</match>
</Operation>

</Patch>